{
    "name": "sunaoka/ndjson",
    "description": "A PHP library to read and write NDJSON (Newline Delimited JSON)",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "sunaoka",
            "email": "sunaoka@pocari.org"
        }
    ],
    "require": {
        "php": ">=5.5",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": ">=4"
    },
    "autoload": {
        "psr-4": {
            "Sunaoka\\Ndjson\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
